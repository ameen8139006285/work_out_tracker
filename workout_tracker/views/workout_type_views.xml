<?xml version="1.0"?>
<odoo>
    <record id="view_workout_type_form" model="ir.ui.view">
        <field name="name">workout.type.form</field>
        <field name="model">workout.type</field>
        <field name="arch" type="xml">
            <form string="Workout Type">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_workout_type_tree" model="ir.ui.view">
        <field name="name">workout.type.tree</field>
        <field name="model">workout.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_workout_type" model="ir.actions.act_window">
        <field name="name">Workout Types</field>
        <field name="res_model">workout.type</field>
        <field name="view_mode">tree,form</field>
    </record>
    
</odoo>